{
  "name": "smcrow/laravel-container-info",
  "description": "Artisan commands for getting information about the IoC container.",
  "minimum-stability": "stable",
  "keywords": ["laravel", "artisan", "container"],
  "license": "MIT",
  "authors": [
    {
      "name": "Steve Crow",
      "email": "steve@smcrow.net"
    }
  ],
  "require": {
    "php": ">=7.0.2",
    "illuminate/console": "~5.0",
    "illuminate/support": "~5.0",
    "illuminate/container": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Smcrow\\ContainerInformation\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "^0.9.9"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Smcrow\\ContainerInformation\\ContainerInformationServiceProvider"
      ]
    }
  }
}
