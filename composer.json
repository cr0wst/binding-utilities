{
  "name": "smcrow/laravel-container-info",
  "description": "Artisan commands for getting information about the IoC container.",
  "minimum-stability": "stable",
  "keywords": ["laravel", "artisan", "container"],
  "license": "MIT",
  "authors": [
    {
      "name": "Steve Crow",
      "email": "steve@smcrow.net"
    }
  ],
  "require": {
    "php": ">=7.0.2",
    "laravel/framework": "~5.0",
    "symfony/finder": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Smcrow\\ContainerInformation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smcrow\\ContainerInformation\\Testing\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "^0.9.9",
    "squizlabs/php_codesniffer": "^3.1",
    "orchestra/testbench": "^3.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Smcrow\\ContainerInformation\\ContainerInformationProvider"
      ]
    }
  }
}
